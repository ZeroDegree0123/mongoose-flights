<%- include('../partials/header') %>

<h2>Flight List</h2>
<table>
  <thead>
    <tr>
      <th class="airline">Airline</th>
      <th class="airport">Airport</th>
      <th class="flightNo">Flight Number</th>
      <th class="depart">Departs</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
      <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
        </tr>
      <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>